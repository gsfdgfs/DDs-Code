#include <iostream>
using namespace std;
int main(){
int n;
cout<<"enter size of array:";
cin>>n;
int *arr = new int[n];
for(int i=0;i<n;i++)
{
    cout <<"enter the element at arr["<<i<<"]index:";
    cin>>arr[i];
    
}
cout<<endl<<"entered element are:"<<endl;
for(int i=0;i<n;i++){
    cout<<arr[i]<<endl;
}
}
